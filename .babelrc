{
    "presets": [
        [
            "env",
            {
                "targets": {
                    "browsers": [
                        "last 2 versions",
                        "IE 9"
                    ]
                },
                "modules": false,
                "useBuiltIns": true
            }
        ],
        "react"
    ],
    "plugins": [
        "transform-object-rest-spread",
        [
            "transform-runtime",
            {
                "polyfill": false,
                "regenerator": true
            }
        ],
        "react-hot-loader/babel"
    ],
    "env": {
        "production": {
            "plugins": [
                "transform-react-constant-elements",
                "transform-react-inline-elements",
                "transform-react-remove-prop-types"
            ]
        }
    }
}
