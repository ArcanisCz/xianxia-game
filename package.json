{
  "name": "xianxia-game",
  "version": "1.0.14",
  "description": "xianxia-game",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --env.dev --mode development",
    "lint": "eslint --cache --quiet --ext .js src",
    "lintf": "eslint --cache --quiet --ext .js src --fix",
    "prebuild": "rimraf ./dist",
    "build": "npm run build:app && npm run storybook:build",
    "build:app": "cross-env NODE_ENV=production webpack-cli --mode production --env",
    "postversion": "git push && git push --tags",
    "deploy": "gulp deploy",
    "test:base": "cross-env NODE_ENV=test mocha --require @babel/register testSetup.js \"src/**/*.spec.js\"",
    "test": "npm run test:base -- --reporter spec",
    "test:ci": "npm run test:base -- --reporter mocha-junit-reporter",
    "test:watch": "npm run test:base -- --reporter min --watch",
    "storybook": "start-storybook -p 6006 -c .storybook --quiet --ci",
    "storybook:build": "build-storybook -o dist/storybook -c .storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+git@github.com:ArcanisCz/xianxia-game.git"
  },
  "author": "ArcanisCz",
